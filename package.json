{
  "name": "@etalab/edigeo-parser",
  "version": "0.6.4",
  "description": "Blazing fast parser for EDIGEO files",
  "main": "./lib",
  "repository": "git@github.com:etalab/edigeo-parser.git",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "bin": {
    "edigeo-parse": "bin/cli.js"
  },
  "scripts": {
    "lint": "xo",
    "test": "nyc ava",
    "codecov": "codecov -f coverage/coverage-final.json"
  },
  "dependencies": {
    "@turf/boolean-clockwise": "^5.1.5",
    "@turf/boolean-point-in-polygon": "^5.1.5",
    "@turf/helpers": "^5.1.5",
    "@turf/length": "^5.1.5",
    "@turf/line-slice": "^5.1.5",
    "@turf/point-to-line-distance": "5.1.5",
    "debug": "^3.1.0",
    "decompress": "^4.2.0",
    "epsg-index": "^0.7.0",
    "get-stdin": "^5.0.1",
    "is-buffer": "^1.1.6",
    "lodash": "^4.17.4",
    "proj4": "^2.4.4",
    "split2": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "codecov": "^3.0.0",
    "nyc": "^11.4.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "curly": "off",
      "unicorn/no-process-exit": "off"
    }
  },
  "engines": {
    "node": ">= 8"
  }
}
